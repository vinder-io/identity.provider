{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Settings": {
    "Database": {
      "ConnectionString": "${Settings__Database__ConnectionString}",
      "DatabaseName": "${Settings__Database__DatabaseName}"
    },
    "Security": {
      "SecretKey": "${Settings__Security__SecretKey}"
    },
    "Administration": {
      "Username": "${Settings__Administration__Username}",
      "Password": "${Settings__Administration__Password}"
    }
  }
}
